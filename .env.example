# Configurações do Bot Telegram
TELEGRAM_BOT_TOKEN=seu_token_do_telegram_aqui

# Token da Hugging Face (GRATUITO)
# Obtenha em: https://huggingface.co/settings/tokens
HUGGINGFACE_API_TOKEN=hf_xxxxxxxxxxxxxxxxxxxx

# Modelo de IA (opcional)
# Padrão: stabilityai/stable-diffusion-xl-base-1.0 (melhor qualidade)
# Alternativas: 
#   - stabilityai/stable-diffusion-2-1 (mais rápido)
#   - runwayml/stable-diffusion-v1-5 (clássico)
HF_MODEL=stabilityai/stable-diffusion-xl-base-1.0

# Prompt negativo (o que NÃO incluir nas imagens)
NEGATIVE_PROMPT=blurry, bad quality, distorted, ugly, watermark, text

# Configurações de qualidade (opcional)
IMAGE_WIDTH=1024
IMAGE_HEIGHT=1024
INFERENCE_STEPS=30
GUIDANCE_SCALE=7.5
